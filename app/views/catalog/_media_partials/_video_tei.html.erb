<div class="datastream primary-datastream datastream-video datastream-video-mp4">
  <%= render_video_player(datastream_url(document.fedora_object.datastream["Video.mp4"]), :size => "320x240", :controls => true, :poster => datastream_url(document.fedora_object.datastream["Thumbnail"])) %>
</div>

<div class="secondary-datastream-container">
<div class="datastream secondary-datastream datastream-transcript datastream-transcript-tei-xml">
  <%= render_tei_transcript(datastream_url(document.fedora_object.datastream["Transcript.tei.xml"], :private => true)) %>
</div>
</div>
